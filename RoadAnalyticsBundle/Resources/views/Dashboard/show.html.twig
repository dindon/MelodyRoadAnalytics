{% extends 'MelodyRoadAnalyticsBundle:Layout:layout.html.twig' %}
{% block interval %}
	<p class="changeDateContainer">
		Statistiques pour les dates du 
		<input type="text" class="changeDate" id="changeDateStart" value="{{ d1|date('d/m/Y') }}" readonly="readonly" />
		 au 
		<input type="text" class="changeDate" id="changeDateEnd" value="{{ d2|date('d/m/Y') }}" readonly="readonly" />
	</p>
	<input type="hidden" id="dashboardRoute" value="{{ path('MelodyRoadAnalyticsBundle_dashboard') }}" />
{% endblock %}
{% block jquery %}<script type="text/javascript" src="{{ asset('bundles/melodyroadanalytics/js/dash.js') }}"></script>{% endblock %}
{% block body %}
	<div id="middle_dash">
		{% include 'MelodyRoadAnalyticsBundle:Dashboard:body.html.twig' %}
	</div>
{% endblock %}
